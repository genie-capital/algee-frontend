{
  "name": "magic-patterns-vite-template",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "npx vite build",
    "build:production": "NODE_ENV=production npx vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx || echo 'ESLint config not found, skipping linting'",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix || echo 'ESLint config not found, skipping linting'",
    "lint:check": "test -f .eslintrc.json || test -f .eslintrc.js || test -f eslint.config.js",
    "preview": "npx vite preview",
    "preview:production": "npx vite build && npx vite preview",
    "test:build": "npm run build",
    "test:build-with-lint": "npm run lint && npm run build",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "lucide-react": "^0.441.0",
    "magic-patterns-vite-template": "file:",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.18",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "latest",
    "eslint": "^8.50.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "postcss": "latest",
    "tailwindcss": "3.4.17",
    "terser": "^5.40.0",
    "typescript": "^5.5.4",
    "vite": "^5.2.0"
  }
}
